  "ConnectionStrings": {
     "DefaultConnection": "Host=${DB_HOST};Database=${DB_DATABASE};Username=${DB_USERNAME};Password=${DB_PASSWORD};Port=${DB_PORT};SslMode=Require;TrustServerCertificate=True;"
  },
